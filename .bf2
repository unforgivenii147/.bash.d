#!/bin/sh

gpp() {
	g++ -std=c++17 -Wall -Wextra -O2 -fPIE -pie "$@" -o out
}

tcc() {

    echo "copy file content to clipboard using termux-api"
    FNAME="$@"
    cat "${FNAME}" | termux-clipboard-set
    echo "done"
}

dir(){
    ls -F --color=auto  -lF --color=always  "$@" | less -r
}

g11() {
    gh repo clone "$@" -- --depth 1 --single-branch --branch master
}
g12() {
    gh repo clone "$@" -- --depth 1 --single-branch --branch main
}
